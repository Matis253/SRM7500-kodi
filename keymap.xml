<keymap>
  <global>
    <remote>
      <playdvd>contextmenu</playdvd>
      <red>title</red>
      <green>chapter</green>
      <yellow>subtitles</yellow>
      <blue>audio</blue>
      <WORKINPROGRESS>Please don't download this now</WORKINPROGRESS>
    </remote>
  </global>
</keymap>
